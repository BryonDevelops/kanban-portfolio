# Copy to devcontainer.env on your host and fill in secrets before building the dev container.
# These values are injected during container build/postCreate so npm install and scripts can access them.
TIPTAP_PRO_TOKEN=

# Optional: surface auth keys inside the container.
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Supabase service role key (needed for migrations/scripts).
SUPABASE_SERVICE_ROLE_KEY=

# Email provider (contact form). Safe to leave blank for local dev.
SENDGRID_API_KEY=
SENDGRID_SENDER_EMAIL=
SENDGRID_SENDER_NAME=
CONTACT_RECIPIENT=

# Unsplash API key for the image picker.
NEXT_PUBLIC_UNSPLASH_ACCESS_KEY=

# Tiptap collaboration / AI configuration (optional).
NEXT_PUBLIC_TIPTAP_COLLAB_DOC_PREFIX=
NEXT_PUBLIC_TIPTAP_COLLAB_APP_ID=
NEXT_PUBLIC_TIPTAP_COLLAB_TOKEN=
NEXT_PUBLIC_TIPTAP_AI_APP_ID=
NEXT_PUBLIC_TIPTAP_AI_TOKEN=
NEXT_PUBLIC_USE_JWT_TOKEN_API_ENDPOINT=

