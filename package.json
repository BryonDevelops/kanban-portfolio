    "cypress": "cypress open --config-file tests/e2e/config/cypress.config.js",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "storybook": "storybook dev -p 6006 -c tests/presentation/storybook/stories/.storybook",
    "build-storybook": "storybook build -c tests/presentation/storybook/stories/.storybook",
